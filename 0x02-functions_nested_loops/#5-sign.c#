
#include <stdio.h>

int print_sign(int n);

/**
 *main: uses print_sign
 *Return - 0
 */

int main(void)
{
int r;
r = _isalpha('H');
putchar(r + '0');
r = _isalpha('o');
putchar(r + '0');
r = _isalpha(108);
putchar(r + '0');
r = _isalpha(';');
putchar(r + '0');
putchar('\n');
return (0);
}

/**
 *print_sign: prints the sigsn or a number
 *Return - 1 or 0
 */
int print_sign(int n)
{
if ((c >= 65 && c <= 90) || (c >= 97 && c <= 122))
{
return 1;
}
return 0;
}
