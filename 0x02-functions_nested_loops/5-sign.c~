
#include <stdio.h>

int print_sign(int n);

/**
 *main: calls print_sign
 *Return - 0
 */

int main(void)
{
int r;
r = print_sign('H');
putchar(r + '0');
r = print_sign('o');
putchar(r + '0');
r = print_sign(108);
putchar(r + '0');
r = print_sign(';');
putchar(r + '0');
putchar('\n');
return (0);
}

/**
 *_islower: checks if a letter is in upercase
 *Return - 1 or 0
 */
int print_sign(int n)
{
if (n > 0)
{
  putchar(1 + '0')
return 1;
}
return 0;
}
