#include "holberton.h"

int div(int n);
/**
 *_sqrt_recursion - returns the natural square root of a number
 *@n: number
 *Return: square root on n
 */
int _sqrt_recursion(int n)
{
int x;
int y;
x = n * n;
y = x/n //4 times
  
if (n % y != 0)
return (-1);

return (x * _pow_recursion(x, (y - 1)));
}

